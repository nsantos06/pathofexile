#include <stdio.h>
#include <stdlib.h>
int main(){
	int x,y;
	printf("Escolha um numero inteiro: \n");
	scanf("%d",&x);
	printf("Escolha outro numero intero: \n");
	scanf("%d",&y);
	if(x>y){
		printf("O maior numero e:%d",x);
}
	else{
		printf("O Maior numero e:%d",y);
	}
		return 0;
	}
	
	
	
	
	
	
	
